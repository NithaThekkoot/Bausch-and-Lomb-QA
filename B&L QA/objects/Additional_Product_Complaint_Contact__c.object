    <sharingModel>ReadWrite</sharingModel>
    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>ProductComplaintAddlContact_CaseClosed</fullName>
        <active>true</active>
        <description>If the related Case is closed, do not allow changes to the connection between the Additional Contact (Account) and the Case.  The Additional Contact itself can still be modified.</description>
        <errorConditionFormula>and( 
  $User.Profile__c &lt;&gt; &quot;BL: Integration User&quot;, 
  IsPickVal(Case__r.Status, &quot;Closed&quot;),
  NOT(IsNull(Case__r.Complaint_Action_ID__c))
)</errorConditionFormula>
        <errorMessage>Once a Product Complaint has been closed, no additional changes may be made. The Product Complaint can be reopened in CATSWeb.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductComplaintAddlContact_PhoneTooLong</fullName>
        <active>true</active>
        <description>For Product Complaints, CATSWeb only accepts 30 characters for Phone</description>
        <errorConditionFormula>LEN( Phone__c ) &gt; 30</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>This field can only contain 30 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductComplaint_NASU_AddCntct_RFE</fullName>
        <active>true</active>
        <description>For Product Complaints, once the related Case has been sent to CW, all edits must have a Reason for Edit.</description>
        <errorConditionFormula>and (
  Not($User.Profile__c = &quot;BL: Integration User&quot;),
  Not(IsNew()),
  Case__r.CATSWeb_Complaint_Number__c &lt;&gt; &quot;&quot;,
  Reason_for_Edit_Prior_Value__c = &quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>Reason_for_Edit__c</errorDisplayField>
        <errorMessage>All changes to Additional Contacts require a Reason for Edit once the Product Complaint has been sent to CATSWeb.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
