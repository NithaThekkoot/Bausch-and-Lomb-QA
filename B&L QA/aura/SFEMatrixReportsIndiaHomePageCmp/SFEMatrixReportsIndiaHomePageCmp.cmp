<aura:component implements="flexipage:availableForAllPageTypes" access="global" >
    <article class="slds-card">
    	<div class="slds-card__header slds-grid">
        	<header class="slds-media slds-media_center slds-has-flexi-truncate">
      			<div class="slds-media__figure">
        			<span class="slds-icon_container slds-icon-standard-contact" title="description of icon when needed">
          				<lightning:icon iconName="standard:home"/>
        			</span>
      			</div>
      			<div class="slds-media__body">
        			<h2>
          				<a href="javascript:void(0);" class="slds-card__header-link slds-truncate" title="[object Object]">
            				<span class="slds-text-heading_small">SFE Matrix Reports (India)</span>
          				</a>
        			</h2>
      			</div>
    		</header>
        </div>
        <div class="slds-card__body slds-card__body_inner">
            <ul class="slds-list--vertical slds-has-block-links">
                <li class="slds-p-horizontal--small"><c:GenericCustomLinkCmp urlValue="/apex/SFECallingReport" linkLabel="SFE Calling Report"/></li>
                <li class="slds-p-horizontal--small"><c:GenericCustomLinkCmp urlValue="/apex/SFECallingMatrixCustomReport" linkLabel="SFE Calling Matrix Report"/></li>
                <li class="slds-p-horizontal--small"><c:GenericCustomLinkCmp urlValue="/apex/SFEDailyCallingMatrixCustomReport" linkLabel="SFE Daily Calling report"/></li>
                <li class="slds-p-horizontal--small"><c:GenericCustomLinkCmp urlValue="/apex/SFEMonthlyAvgMatrixCustomReport" linkLabel="SFE Monthly Average Report"/></li>
                <li class="slds-p-horizontal--small"><c:GenericCustomLinkCmp urlValue="/apex/SFEPlanMatrixCustomReport" linkLabel="SFE Plan Matrix Report"/></li>
            </ul>
        </div>
    </article>
</aura:component>